{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src/",
    "paths": {
      "@screens/*": ["screens/*"],
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"],
      "@navigations/*": ["navigations/*"],
      "@typs/*": ["typs/*"],
      "@redux/*": ["redux/*"],
      "@utils/*": ["utils/*"],
